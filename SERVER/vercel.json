{
    "version": 2,
    "env": {
    },
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/*",
            "dest": "/"
        },
        {
            "src": "/(.+)",
            "dest": "src/main.js"
        }
    ]
}